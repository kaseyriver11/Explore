data <- read.sas7bdat("C:/Users/kaseyriver11/Google Drive/IAA_Insurance/c_adjustor_technician_full.SAS7bdat")
Adjustor <- as.character(unique(data$Adjustor))
adjustordata <- data.frame( Claims=numeric(), Claims_Rewarded=numeric(),
Claims_Excluded = numeric(), AverageReward=numeric(), First_Claim = numeric(),
Last_Claim = numeric())
for(i in 1:length(Adjustor)){
filter <- subset(data, data$Adjustor == Adjustor[i])
First_Claim <- min(filter$Date)
Last_Claim <- max(filter$Date)
AverageReward <- mean(filter$Reward_A[filter$Reward_A>0])
Claims <- nrow(filter)
Claims_Rewarded <- nrow(filter[filter$Reward_A>0,])
Claims_Excluded <- Claims - Claims_Rewarded
Row <- c(Claims, Claims_Rewarded, Claims_Excluded, AverageReward, First_Claim, Last_Claim)
adjustordata[i,] <- Row
}
adjustordata <- cbind(Adjustor, adjustordata)
